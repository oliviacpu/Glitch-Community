.grid
  display: grid  
  grid-row-gap: var(--row-gap, 10px)
  grid-column-gap: var(--column-gap, 10px)
  grid-template-columns: repeat(auto-fill, minmax(var(--min-width, 300px), 1fr))
  list-style-type: none
  margin: 0
  padding: 0

.item
  list-style: none


.dragStyle{ 
  box-shadow: 0px 7px 6px -7px rgba(0,0,0,0.27)}
